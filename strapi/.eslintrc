{
  "parser": "@babel/eslint-parser",
  "extends": "eslint:recommended",
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": false
  },
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": [ "@babel/preset-react" ]
    },
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": false
    },
    "sourceType": "module"
  },
  "globals": {
    "strapi": true
  },
  "rules": {
        "no-unused-vars": "off"
      }
}
